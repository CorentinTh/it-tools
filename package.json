    "less": "^4.1.2",
    "prettier": "^2.6.2",
    "standard-version": "^9.3.2",
    "start-server-and-test": "^1.14.0",
    "typescript": "~4.5.5",
    "vite": "^2.9.1",
    "vite-plugin-md": "^0.12.4",
    "vite-plugin-pwa": "^0.11.13",
    "vite-svg-loader": "^3.2.0",
    "vitest": "^0.13.1",
    "vue-tsc": "^0.31.4"
  }
